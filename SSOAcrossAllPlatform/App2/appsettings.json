{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "Authority": "http://localhost:8080/realms/sso-realm",
    "ClientId": "app2-client",
    "ClientSecret": "app2-client-secret",
    "RequireHttpsMetadata": false,
    "ResponseType": "code",
    "Scope": "openid profile email",
    "SaveTokens": true,
    "GetClaimsFromUserInfoEndpoint": true
  },
  "SSOSettings": {
    "CommonLoginUrl": "http://localhost:5000",
    "RequiredRoles": ["app2-user", "admin"]
  }
}